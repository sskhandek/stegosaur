{
    "name": "Stegosaur",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Injecting stuff",
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "jquery-3.1.1.min.js",
        "gmail.js",
        "main.js"
    ]
}