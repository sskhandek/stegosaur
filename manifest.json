{
    "name": "Stegosaur",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "PGP + Steganography!",
    "homepage_url": "illinois.edu",
    "background": {
        "scripts": [
            "main.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Inject!"
    },
    "permissions": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*",
        "tabs"
    ]
}